<% if @user.errors.any? %>
    <div id="error_explanation">
        <div class="alert alert-danger">
            <%= @user.errors.count%>点のエラーがあります
        </div>
        <%= params %>
        <ul>
        <% @user.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end %>
        </ul>
    </div>
<% end %>